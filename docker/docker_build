#!/bin/bash

set -x
docker container rm qlmux_proxy --force --volumes 
docker image rm --force qlmux_proxy
docker build --tag qlmux_proxy .

docker network insepct qlmux_proxy>/dev/null 2>&1 || docker network create qlmux_proxy 

exit 0
docker run --hostname qllabels.local \
	--network qlmux_proxy \
	--publish 9100-9104:9100-9104 \
	--publish 5000:5000 \
	--restart always \
	--env PYTHONUNBUFFERED=1 \
	-t -i --name qlmux_proxy qlmux_proxy /bin/bash

